public with sharing class calculator {
    public calculator() {
    }
    public integer addition(integer a, integer b){
        return a+b; //addition feature is added
    }
    public integer sub(integer a, integer b){
        return a-b; //subtracting feature
    }
    public integer multiplecation(integer a, integer b){
        return a*b; //multiplication feature is added
    }
}
